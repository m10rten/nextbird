{
  "name": "nextbird",
  "version": "0.0.1",
  "description": "NextBird is a simple, lightweight and fast library for creating a status page for your services.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests"
  },
  "keywords": [
    "status",
    "healthcheck",
    "statuspage",
    "Nextjs",
    "Next",
    "React",
    "Node",
    "Typescript",
    "Typesafe",
    "uptime",
    "monitoring"
  ],
  "author": "m10rten",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "next": "14.0.1",
    "@nextbird/action": "0.0.1",
    "@nextbird/fetch": "0.0.1"
  },
  "peerDependencies": {
    "next": "^13.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "zod": "3.22.4"
  }
}
