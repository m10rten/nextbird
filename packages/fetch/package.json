{
  "name": "@nextbird/fetch",
  "version": "0.0.0",
  "description": "NextBird is a simple, lightweight and fast library for creating a status page for your services.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests"
  },
  "keywords": [
    "status",
    "healthcheck",
    "statuspage",
    "Nextjs",
    "Next",
    "React",
    "Node",
    "Typescript",
    "Typesafe",
    "uptime",
    "monitoring"
  ],
  "author": "m10rten",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./fetcher": {
      "import": "./dist/fetcher/index.mjs",
      "require": "./dist/fetcher/index.js",
      "types": "./dist/fetcher/index.d.ts"
    }
  },
  "devDependencies": {},
  "peerDependencies": {},
  "dependencies": {
    "zod": "^3.22.4"
  }
}
